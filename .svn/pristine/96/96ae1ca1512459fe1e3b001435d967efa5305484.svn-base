

<wicket:extend>
	<form wicket:id="form"  class="ui form">
		<table>
			<tr>
				<td>
					<div class="field">
						<label><span wicket:id="clazzNameLabel" /></label> <select wicket:id="clazzName"></select>
					</div>
				</td>
			</tr>
			<tr>
				<td>
					<div class="field">
						<label><span wicket:id="primaryKeyLabel" /></label> <input wicket:id="primaryKey"></input>
					</div>
				</td>
			</tr>
			<tr>
				<td>
					<div class="field">
						<label><span wicket:id="filterFieldLabel" /></label> <input wicket:id="filterField"></input>
					</div>
				</td>
			</tr>
			<tr><td>&nbsp;</td></tr>
			<tr>
				<td><input wicket:id="search" type="button" class="ui primary button" value="Search" /></td>
			</tr>
		</table>
		<div wicket:id="wmcSearchResult">
			<table class="ui celled structured table">
			  <thead>
			    <tr>
			    	<th wicket:id="sortingHeader">
			    		<span wicket:id="column">[column]</span>
			    	</th>
			  	</tr>
			  </thead>
			  <tbody>
			    <tr wicket:id="sortingRows">
					<td wicket:id="objValues"><span wicket:id="column">[column]</span></td>
				</tr>
			  </tbody>
			  <tfoot>
			    <tr>
			    	<th colspan="100">
			    		<!--span wicket:id="navigatorLabel">[dataView navigatorLabel]</span>
				        <span wicket:id="navigator">[dataView navigator]</span-->
			    	</th>
			  </tr>
			  </tfoot>
			</table>
		</div>
	</form>
</wicket:extend>
	