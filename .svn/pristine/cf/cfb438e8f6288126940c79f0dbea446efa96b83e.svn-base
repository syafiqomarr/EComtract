<wicket:extend>
	<form wicket:id="form" class="ui form">
		<div class="ui segments">
		
			<div class="ui horizontal segments">
					<div class="ui segment">
						<div class="field">
							<label><span wicket:id="okuRefNoLabel" /></label> <input type="text" wicket:id="okuRefNo">
						</div>
						<div class="field">
							<label><span wicket:id="okuCardNoLabel" /></label> <input type="text" wicket:id="okuCardNo">
						</div>
						<div class="two fields">
							<div class="field">
								<label><span wicket:id="idTypeLabel" /></label> <select wicket:id="idType" style="width: 150;"></select>
							</div>
							<div class="field">
								<label><span wicket:id="idNoLabel" /></label> <input wicket:id="idNo" style="width: 150;" />
							</div>
						</div>
					</div>
					
					<div class="ui segment">
						<div class="field">
							<label><span wicket:id="okuCategoryLabel" /></label> <select wicket:id="okuCategory"></select>
						</div>
						
						<div class="field">
							<label><span wicket:id="okuRegStatusLabel" /></label> <select wicket:id="okuRegStatus"></select>
						</div>
						<div class="two fields">
							<div class="field">
								<label><span wicket:id="createDtFromLabel" /></label> <input wicket:id="createDtFrom" style="width: 150;" />
							</div>
							<div class="field">
								<label><span wicket:id="createDtToLabel" /></label> <input wicket:id="createDtTo" style="width: 150;" />
							</div>
						</div>
					</div>
			</div>
				
				<div class="ui segment">
					<input wicket:id="searchBtn" type="button" class="ui primary button"  />
				</div>
		</div>
		
		<div wicket:id="wmcSearchResult">
			<table class="ui striped table">
			  <thead>
			    <tr>
			    	<th width="15%"><wicket:message key="page.lbl.user.profile.oku.okuRefNo" /></th>
					<th width="30%"><wicket:message key="page.lbl.user.profile.oku.name" /></th>
					<th width="15%"><wicket:message key="page.lbl.user.profile.oku.idNo" /></th>
					<th width="15%"><wicket:message key="page.lbl.user.profile.oku.applicationDt" /></th>
					<th width="15%"><wicket:message key="page.lbl.user.profile.oku.okuRegStatus" /></th>
					<th width="15%"><wicket:message key="page.lbl.user.profile.oku.approvalDt" /></th>
					<th width="15%"><wicket:message key="page.lbl.user.profile.oku.approveBy" /></th>
					<th width="15%"><wicket:message key="page.lbl.user.profile.oku.createDt" /></th>
					<th width="15%"><wicket:message key="page.lbl.user.profile.oku.updateDt" /></th>
					<th width="10%"><wicket:message key="page.lbl.user.profile.oku.actionSearch" /></th>
			  	</tr>
			  </thead>
			  
			  <tbody>
			    <tr wicket:id="sorting">
					<td><span wicket:id="okuRefNo">[okuRefNo]</span></td>
					<td><span wicket:id="name">[name]</span></td>
					<td><span wicket:id="idNo">[idNo]</span></td>
					<td><span wicket:id="applicationDt">[applicationDt]</span></td>
					<td><span wicket:id="okuRegStatus">[okuRegStatus]</span></td>
					<td><span wicket:id="approvalDt">[approvalDt]</span></td>
					<td><span wicket:id="approveBy">[approveBy]</span></td>
					<td><span wicket:id="createDt">[createDt]</span></td>
					<td><span wicket:id="updateDt">[updateDt]</span></td>
					<td class="manage-link">
						 <a wicket:id="detailLink" href="#" wicket:message="title:page.lbl.user.profile.oku.detailLink"><i class="Edit icon" /></a>
					</td>
				</tr>
			  </tbody>
			  
			  <tfoot>
			    <tr>
			    	<th colspan="100">
			    		<span wicket:id="navigatorLabel">[dataView navigatorLabel]</span>
				        <span wicket:id="navigator">[dataView navigator]</span>
			    	</th>
			  	</tr>
			  </tfoot>
			</table>
		</div>
	</form>
</wicket:extend>

