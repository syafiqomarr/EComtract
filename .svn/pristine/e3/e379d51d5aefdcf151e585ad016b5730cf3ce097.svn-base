<wicket:extend>
	<form wicket:id="form" >
		<div id="PaymentCheckWithEghl" class="ui form segment">
			<table>
				<tr>
					<td>
						<div class="ui top attached segment">
							<div class="ui top attached label">
								<h4 class="ui header">
									<wicket:message key="page.lbl.ezbiz.otcPayment.paymentSearch" />
								</h4>
							</div>
							<table>
								<tr>
									<td>
										<div class="field">
											<label><span wicket:id="appRefNoLabel" /></label> <input wicket:id="appRefNo" style="width: 400;"></input>
										</div>
									</td>
								</tr>
<!-- 								<tr>
									<td>
										<div class="field">
											<label><span wicket:id="statusLabel" /></label> <select wicket:id="status">
											</select>
										</div>
									</td>
								</tr> -->
								<tr><td>&nbsp;</td></tr>
								<tr>
									<td><input wicket:id="search" type="button" class="ui primary button" value="Search" /></td>
								</tr>
							</table>
						</div>
					</td>
				</tr>
			</table>
		</div>
		<div wicket:id="wmcSearchResult">
			<table class="ui striped table">
			  <thead>
			    <tr>
			    	<th width="100%"><wicket:message key="page.lbl.ezbiz.robFormA.eghlResponse" /></th>
			  	</tr>
			  </thead>
			  <tbody>
			    <tr wicket:id="sorting">
					<td><span wicket:id="eghlResponse">[eghlResponse]</span></td>
				</tr>
			  </tbody>
			  <tfoot>
			    <tr>
			    	<th colspan="100">
			    		<span wicket:id="navigatorLabel">[dataView navigatorLabel]</span>
				        <span wicket:id="navigator">[dataView navigator]</span>
			    	</th>
			  </tr>
			  </tfoot>
			</table>
		</div>
	</form>
</wicket:extend>