 <style type="text/css">
     th.last-name *, td.last-name {
         font-weight: bold;
         color: #AA0000;
     }
     th.numeric, td.numeric {
         text-align: right !important;
     }
 </style>
<wicket:extend>
<div id="llpReservedNames">
	<span wicket:id="searchLlpName"></span>
	<table class="blockdataview" cellspacing="0" >
		  <tr>
			<td align="left"><span wicket:id="navigatorLabel">[dataview navigatorLabel]</span></td>
			<td align="right"><span wicket:id="navigator">[dataview navigator]</span></td>
		</tr>
		<tr>
			<td colspan="2">
				<table cellspacing="0" class="dataview" width="100%">
				    <tr>
				        <!-- <th>ID</th>-->
				       <!--  <th wicket:id="orderByResultRef">Result Reference</th>-->
				        <th>LLP Name</th>
				        <th>LLP No</th>
				        <th>Reference No</th>
				        <!-- <th>Result Date</th>
				        <th>Name Expired Date</th>
				        <th>Action</th>-->
				    </tr>
				    <tr wicket:id="sorting">
				        <!-- <td><span wicket:id="idReservedName">[idReservedName]</span> </td>-->
				        <!--  <td><span wicket:id="refNo">[refNo]</span> </td>-->
				        <td><span wicket:id="applyLlpName">[applyLlpName]</span></td>			        
				        <td><span wicket:id="llpNo">[llpNo]</span></td>
				        <td><span wicket:id="refNo">[refNo]</span> </td>
				        <!-- <td><span wicket:id="status">[status]</span></td>-->
				        <!-- <td><span wicket:id="resultDate">[resultDate]</span></td>
				        <td><span wicket:id="expNameDate">[expNameDate]</span></td>-->
				        <td class="manage-llpReservedName-links">
			                  <!-- <a wicket:id="edit" href="#">[edit]</a>
			                   <a wicket:id="delete" href="#">[delete]</a>
			                   <a wicket:id="llpRegistration" href="#">[LLP Registration]</a>-->
			            </td>
				    </tr>				    
				</table>					
			</td>
		</tr>
	</table>
	<form wicket:id="form">
			<table>
			     <tr>
					<td colspan="2"><font color="green" size="4"><wicket:message key="reservedName.page.nameAvailable" /></font></td>
				</tr>
				<tr>
					<td><wicket:message key="reservedName.page.applyLlpName" /></td>
					<td><input wicket:id="applyLlpName" type="text" size="100" readonly="readonly"/></td>
				</tr>
				<tr>
					<td colspan="2">&nbsp;</td>
				</tr>
				<wicket:enclosure child="guidelinesLink">
				<tr>
					<td colspan="2"><b><wicket:message
								key="reservedName.page.guideline.declaration.lbl" /> </b>
					</td>
				</tr>
				<tr>
					<td colspan="2"><input wicket:id="declareChk" type="checkbox" />
						<wicket:message key="reservedName.page.guideline.declaration">
						    <a wicket:id="guidelinesLink"><wicket:message key="reservedName.page.guidelinesLinkLabel"/></a>
						</wicket:message>	
					</td>
				</tr>
				</wicket:enclosure>
				<tr>
					<td colspan="2">
					<br>
						<input wicket:id="reserve" type="submit" wicket:message="value:reservedName.page.buttonReserve" />
						<input wicket:id="proceedName" type="submit" wicket:message="value:reservedName.page.buttonProceedName" />
					</td>
				</tr>
			</table>
			<input wicket:id="regType" type="hidden" />
			<input wicket:id="profBodyType" type="hidden" />
			<input wicket:id="isProceedToLLP" type="hidden" />
			<input wicket:id="conversionType" type="hidden" />
	</form>
</div>
</wicket:extend>