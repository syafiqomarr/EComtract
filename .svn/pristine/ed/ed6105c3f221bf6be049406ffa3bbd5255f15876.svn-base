<wicket:extend>
	<form wicket:id="pRegForm">
		<div id="llpPartnerLink">
			<div class="llpPartnerLink">
				<table>

					
					
					<table >
				  	<tr>
						<td><wicket:message key="llpReg.page.partner.name"/></td>
						<td colspan="4"><input wicket:id="llpUserProfile.name" type="text" size="101" /></td>
					</tr>

					<tr>
						<td><wicket:message key="llpReg.page.partner.idType"/></td>
						<td colspan = "4"><select wicket:id="llpUserProfile.idType" /></td>
						
					</tr>

					<tr>
						<td><wicket:message key="llpReg.page.partner.idNo"/></td>
						<td colspan = "4"><input wicket:id="llpUserProfile.idNo" type="text" size="35" /></td>
						
					</tr>

  
					<tr>
						<td><wicket:message key="llpReg.page.partner.dob"/></td>
						<td colspan="4"><input wicket:id="llpUserProfile.dob" size="35" /></td>
						
					</tr>

					<tr>
						<td><wicket:message key="llpReg.page.partner.nationality"/></td>
						<td colspan="4"><span wicket:id="llpUserProfile.nationality" /></td>
						
					</tr>
					
					
					<tr>
						<td><wicket:message key="llpReg.page.partner.race"/></td>
						<td colspan="4"><select wicket:id="llpUserProfile.race"/></td>
						
					</tr>
				
			</table>
			<hr  color="grey">
			
			
			
			<br>

			<table >
				<tr>
					<td><wicket:message key="llpReg.page.partner.add"/></td>
					<td colspan="4"><input wicket:id="add1" type="text" size="101"/></td>
				</tr>

				<tr>
					<td></td>
					<td colspan="4"><input wicket:id="add2" type="text" size="101"/></td>
				</tr>

				<tr>
					<td></td>
					<td colspan="4"><input wicket:id="add3" type="text" size="101"/></td>
				</tr>

				<tr>
					<td><wicket:message key="llpReg.page.partner.postcode"/></td>
					<td><input wicket:id="postcode" type="text" size="35"/></td>
					<td></td>
					<td><wicket:message key="llpReg.page.partner.city"/></td>
					<td><input wicket:id="city" type="text" size="49"/></td>
				</tr>

				<tr>
					<td><wicket:message key="llpReg.page.partner.state"/></td>
					<td><select wicket:id="state"/></td>
					<td></td>
					<td><wicket:message key="llpReg.page.partner.country"/></td>
					<td><select wicket:id="country"/></td>
				</tr>

				<tr>
					<td><wicket:message key="llpReg.page.partner.offNo"/></td>
					<td><input wicket:id="offNo" type="text" size="35"/></td>
					<td></td>
					<td><wicket:message key="llpReg.page.partner.hpNo"/></td>
					<td><input wicket:id="hpNo" type="text" size="49"/></td>
				</tr>

				<tr>
					<td><wicket:message key="llpReg.page.partner.faxNo"/></td>
					<td><input wicket:id="faxNo" type="text" size="35"/></td>
					<td></td>
					<td><wicket:message key="llpReg.page.partner.email"/></td>
					<td><input wicket:id="llpUserProfile.email" type="text" size="49"/></td>
				</tr>
			</table>
			<hr  color="grey">
			


			<table bgcolor = "grey">
				<tr>
					<td colspan="3" align="center">
					<input wicket:id="save" type="submit" wicket:message="value:llpReg.page.saveButton"/> 
					<input wicket:id="cancel" type="submit" wicket:message="value:llpReg.page.cancelButton"/>
					</td>
				</tr>
			</table>
			
			
			</table>
		</div>
		</div>
	</form>
</wicket:extend>