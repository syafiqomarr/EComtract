<wicket:extend>
	<form wicket:id="generateForm" class="ui form">
		<div class="ui top attached segment">
			<div class="ui top attached label">
				<h4 class="ui header">
					<wicket:message key=base.title.generate.Password />
				</h4>
			</div>			
			<table>
				<wicket:enclosure child="loginId" renderBodyOnly="true">
					<tr><td>&nbsp;</td></tr>
					<tr>
						<td>
							<div class="field">
								<label><span wicket:id="loginIdLabel" /></label>
								<input wicket:id="loginId" type="text" size="50" />
							</div>
						</td>
					</tr>
					<tr><td>&nbsp;</td></tr>
					<tr><td align="center"><b>OR</b></td></tr>
					<tr><td>&nbsp;</td></tr>
				</wicket:enclosure>
				<tr>
					<td>
						<div class="field">
							<label><span wicket:id="idNoLabel" /></label>
							<input wicket:id="idNo" type="text" size="50" />
						</div>
					</td>
				</tr>
				<tr><td>&nbsp;</td></tr>
				<tr>
					<td>
						<div class="inline field">
							<input wicket:id="save" class="ui primary button" type="submit" wicket:message="value:user.page.userGeneratePassword"/> 
							<input wicket:id="cancel" class="ui primary button" type="submit" value="Cancel" />
						</div>
					</td>
				</tr>
			</table>
		</div>
	</form>
</wicket:extend>